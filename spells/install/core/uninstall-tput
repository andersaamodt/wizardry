#!/usr/bin/env sh
set -eu

SCRIPT_DIR=$(CDPATH= cd -- "$(dirname "$0")" && pwd -P)
APT_PACKAGE=ncurses-bin DNF_PACKAGE=ncurses YUM_PACKAGE=ncurses ZYPPER_PACKAGE=ncurses PACMAN_PACKAGE=ncurses APK_PACKAGE=ncurses BREW_PACKAGE=ncurses \
  ASSUME_YES=1 "$SCRIPT_DIR/manage-system-command" --uninstall tput ncurses
