#!/bin/sh
# test-lack NAME REASON - output a test incompleteness warning
# Example: test-lack "my-spell" "missing --help test"

set -eu

_test_lack() {
  name=$1
  reason=$2
  printf 'LACK %s (%s)\n' "$name" "$reason"
}

# Self-execution when run directly (not sourced)
case "${0##*/}" in test-lack) _test_lack "$@" ;; esac
