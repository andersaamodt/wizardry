#!/bin/sh
# stub-move-cursor - test stub for move-cursor (no-op)
# Example: stub-move-cursor 10 20

set -eu

_stub_move_cursor() {
  exit 0
}

# Self-execute when run directly (not sourced); match stub and symlink names
case "$0" in
  */move-cursor|*/stub-move-cursor)
    [ -z "${_WIZARDRY_SOURCING-}" ] && _stub_move_cursor "$@"
    ;;
esac
