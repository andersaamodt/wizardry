#!/bin/sh
# assert-failure STATUS - assert command failed (non-zero status)
# Exits 0 if STATUS is non-zero, otherwise exits 1.
# Example: test-assert-failure "$STATUS" || return 1

set -eu
_status=${1:-0}
if [ "$_status" -eq 0 ]; then
  printf 'expected failure status but got success\n' >&2
  exit 1
fi
