#!/bin/sh
# differs A B - test if two strings are different (opposite of equals)
# Example: differs "$old" "$new" && update


usage() {
        cat <<'USAGE' >&2
Usage: differs A B

Test if two strings are different (opposite of equals). Example: differs "$old" "$new" && update.
USAGE
}

case "${1-}" in
--help|--usage|-h)
        usage
        exit 0
        ;;
esac

[ "$1" != "$2" ]
