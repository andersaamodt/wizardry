#!/bin/sh
# starts STRING PREFIX - test if string starts with prefix
# Example: starts "$line" "#" && echo "comment"
set -eu

_starts() {
  case "$1" in
    "$2"*) return 0 ;;
    *) return 1 ;;
  esac
}

# Self-execute when run directly (not sourced)
set -eu
case "$0" in
  */starts) _starts "$@" ;; esac
