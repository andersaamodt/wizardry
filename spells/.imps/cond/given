#!/bin/sh
# given VALUE - test if string is non-empty (provided)
# DEPRECATED: Use 'nonempty' instead for clarity
# Example: nonempty "$NAME" && echo "Hello, $NAME"


usage() {
        cat <<'USAGE' >&2
Usage: given VALUE

Test if string is non-empty (provided) DEPRECATED: Use 'nonempty' instead for clarity. Example: nonempty "$NAME" && echo "Hello, $NAME".
USAGE
}

case "${1-}" in
--help|--usage|-h)
        usage
        exit 0
        ;;
esac

[ -n "$1" ]
