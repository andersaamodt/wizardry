# Inlined imp: cleanup-file
_cleanup_file() {
  [ -n "${1:-}" ] && [ -f "$1" ] && rm -f "$1"
  return 0
}

# _cleanup_file FILE - remove temp file if it exists (silent on missing)
# Example: trap '_cleanup_file "$tmp"' EXIT

_cleanup_file() {
  [ -n "${1:-}" ] && [ -f "$1" ] && rm -f "$1"
  return 0
}

# Self-execute when run directly (not sourced)
case "$0" in
  */_cleanup_file) _cleanup_file "$@" ;; esac
