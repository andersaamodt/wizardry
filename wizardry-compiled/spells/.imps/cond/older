# older FILE1 FILE2 - test if FILE1 is older than FILE2 (or FILE2 is newer)
# Example: older src.c src.o || compile src.c

_older() {
  [ "$1" -ot "$2" ]
}

# Self-execute when run directly (not sourced)
case "$0" in
  */older) _older "$@" ;; esac
