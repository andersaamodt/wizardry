# nonempty VALUE - test if string is non-empty (has content)
# Example: nonempty "$NAME" && echo "Hello, $NAME"

_nonempty() {
  [ -n "$1" ]
}

# Self-execute when run directly (not sourced)
case "$0" in
  */nonempty) _nonempty "$@" ;; esac
