# Inlined imp: assert-path-missing
_assert_path_missing() {
  if [ -e "$1" ]; then
    TEST_FAILURE_REASON="expected path to be absent: $1"
    return 1
  fi
  return 0
}

# _assert_path_missing PATH - verify path does not exist
# Sets TEST_FAILURE_REASON if path exists.

_assert_path_missing() {
  if [ -e "$1" ]; then
    TEST_FAILURE_REASON="expected path to be absent: $1"
    return 1
  fi
  return 0
}

# Self-execution when run directly (not sourced)
case "${0##*/}" in _assert_path_missing) _assert_path_missing "$@" ;; esac
